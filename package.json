{
  "name": "diamonds-client",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite & npm run codegen:watch",
    "dev:desktop": "npx tauri dev",
    "dev:ios": "npx tauri ios dev 'iPhone 16'",
    "tauri": "npx tauri",
    "lint": "eslint . --fix",
    "prebuild": "npm run codegen",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "codegen": "graphql-codegen-esm --config codegen.ts",
    "codegen:watch": "graphql-codegen-esm --watch '../../bergline/bergline-svr/schema.gql' 'src/**/*.{query,mutation,subscription}.ts'"
  },
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@nuxt/ui": "^3.0.1",
    "@vue/apollo-composable": "^4.2.1",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^6.0.4",
    "lucide-vue-next": "^0.460.0",
    "reka-ui": "^2.1.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/client-preset": "4.7.0",
    "@graphql-codegen/introspection": "4.0.3",
    "@parcel/watcher": "^2.5.1",
    "@tauri-apps/cli": "^2.4.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "typescript": "~5.8.0",
    "vite": "^6.2.2",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  }
}
